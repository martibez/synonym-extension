{
  "manifest_version": 2,
  "name": "Sinônimos",
  "version": "1.0",
  "description": "Encontre sinônimos facilmente!",
  "author": "Bruno Martinez da Camara",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Sinônimos",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'",
  "permissions": ["activeTab", "http://*/*", "https://*/*"
]
}
